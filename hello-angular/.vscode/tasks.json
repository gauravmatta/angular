{
	"version": "2.0.0",
	"command": "tsc",
	"presentation": {
		"echo": true,
		"reveal": "silent",
		"focus": false,
		"panel": "shared",
		"showReuseMessage": true,
		"clear": false
	},
	"options": {
		"shell": {
			"executable": "tsc"
		}
	},
	"tasks": [
		{
			"type": "typescript",
			"tsconfig": "tsconfig.json",
			"problemMatcher": [
				"$tsc"
			],
			"group": "build",
			"label": "tsc: build - tsconfig.json"
		}
	]
}